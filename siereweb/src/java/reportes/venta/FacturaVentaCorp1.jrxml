     vnt_factura."vfct_porcentajeiva",
     vnt_factura."vfct_iva",
     vnt_factura."vfct_saldo",
     vnt_factura."factsr_fecha",
     vnt_factura."factsr_ordencliente",
     vnt_factura."rgvt_id",
     vnt_factura."vfct_id",
     vnt_detevento."vde_obsr",
     vnt_detallecliente."dcln_nombres" AS vnt_detallecliente_dcln_nombres,
     vnt_cliente."cln_id" AS vnt_cliente_cln_id,
     vnt_cliente."cln_identificacion" AS vnt_cliente_cln_identificacion,
     vnt_cliente."cln_nombres" AS vnt_cliente_cln_nombres,
     vnt_cliente."cln_contacto" AS vnt_cliente_cln_contacto,
     vnt_cliente."cln_correoe" AS vnt_cliente_cln_correoe,
     vnt_cliente."cln_nuevo" AS vnt_cliente_cln_nuevo,
     vnt_cliente."cln_diereccion" AS vnt_cliente_cln_diereccion,
     vnt_cliente."cln_celular" AS vnt_cliente_cln_celular,
     vnt_cliente."cln_fijo" AS vnt_cliente_cln_fijo,
     vnt_cliente."tcl_id" AS vnt_cliente_tcl_id,
     vnt_cliente."tdc_id" AS vnt_cliente_tdc_id,
     vnt_cliente."cln_estado" AS vnt_cliente_cln_estado,
     vnt_cliente."cln_fechaproceso" AS vnt_cliente_cln_fechaproceso,
     vnt_cliente."cln_fechanace" AS vnt_cliente_cln_fechanace,
     vnt_cliente."cln_numhijos" AS vnt_cliente_cln_numhijos,
     vnt_cliente."sex_id" AS vnt_cliente_sex_id,
     vnt_cliente."ate_id" AS vnt_cliente_ate_id,
     vnt_cliente."cln_alias" AS vnt_cliente_cln_alias,
     vnt_factura."vfct_numdiaspago" AS vnt_factura_vfct_numdiaspago
FROM
     "vnt_detevento" vnt_detevento INNER JOIN "vnt_factura" vnt_factura ON vnt_detevento."rgvt_id" = vnt_factura."rgvt_id"
     INNER JOIN "vnt_detallecliente" vnt_detallecliente ON vnt_detevento."dcln_id" = vnt_detallecliente."dcln_id"
     INNER JOIN "vnt_cliente" vnt_cliente ON vnt_detallecliente."cln_id" = vnt_cliente."cln_id"
WHERE
     vfct_id = $P{p_FacturaVentaCorp}]]>
